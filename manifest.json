{
  "manifest_version": 2,
  "name": "AsanaTabs React",
  "version": "0.1",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Quickly view Asana tasks from the new tab page.",

  "background": {
    "persistent": true,
    "scripts": ["background.bundle.js"]
  },
  "content_scripts": [],

  "icons": {
    "16": "webpack.16.png",
    "48": "webpack.48.png",
    "128": "webpack.128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "background",
    "tabs",
    "storage",
    "cookies",
    "activeTab",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "*://*.asana.com/*",
    "http://*/*",
    "https://*/*"
  ]
}
